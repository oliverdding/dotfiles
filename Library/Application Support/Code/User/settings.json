{
    "clangd.arguments": [
        "--background-index",
        "--compile-commands-dir=build",
        "-j=12",
        "--query-driver=/usr/bin/clang++",
        "--clang-tidy",
        "--all-scopes-completion",
        "--completion-style=detailed",
        "--function-arg-placeholders",
        "--header-insertion=iwyu",
        "--pch-storage=memory"
    ],
    "clangd.detectExtensionConflicts": false,
    "containers.containers.description": [
        "Ports",
        "Status"
    ],
    "containers.containers.label": "ContainerName",
    "editor.bracketPairColorization.enabled": true,
    "editor.folding": true,
    "editor.fontFamily": "'Iosevka Nerd Font Mono', 'Noto Serif CJK SC'",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.renderLineHighlight": "none",
    "editor.renderWhitespace": "all",
    "extensions.experimental.affinity": {
        "jasew.vscode-helix-emulation": 1
    },
    "files.autoSave": "onWindowChange",
    "files.eol": "\n",
    "files.exclude": {
        "**/.ammonite": true,
        "**/.bloop": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.metals": true,
        "**/.svn": true,
        "**/dist/**": true,
        "**/node_modules": true,
        "**/node_modules/**": true,
        "**/target": true,
        "**/tmp/**": true
    },
    "files.watcherExclude": {
        "**/.ammonite": true,
        "**/.bloop": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.metals": true,
        "**/.svn": true,
        "**/dist/**": true,
        "**/node_modules": true,
        "**/node_modules/**": true,
        "**/target": true,
        "**/tmp/**": true
    },
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.showPushSuccessNotification": true,
    "git.terminalGitEditor": true,
    "github.gitProtocol": "ssh",
    "go.diagnostic.vulncheck": "Imports",
    "go.editorContextMenuCommands": {
        "fillStruct": true
    },
    "go.formatTool": "gofumpt",
    "go.inlayHints.assignVariableTypes": true,
    "go.inlayHints.compositeLiteralFields": true,
    "go.inlayHints.compositeLiteralTypes": true,
    "go.inlayHints.constantValues": true,
    "go.inlayHints.functionTypeParameters": true,
    "go.inlayHints.parameterNames": true,
    "go.inlayHints.rangeVariableTypes": true,
    "go.lintTool": "revive",
    "go.showWelcome": false,
    "go.survey.prompt": false,
    "go.toolsManagement.autoUpdate": true,
    "gopls": {
        "ui.completion.usePlaceholders": true,
        "ui.diagnostic.analyses": {
            "shadow": true,
            "unusedvariable": true,
            "useany": true
        },
        "ui.diagnostic.staticcheck": true,
        "ui.semanticTokens": true
    },
    "leetcode.defaultLanguage": "golang",
    "leetcode.endpoint": "leetcode-cn",
    "leetcode.filePath": {
        "default": {
            "filename": "${id}.${kebab-case-name}.${ext}",
            "folder": ""
        },
        "golang": {
            "filename": "${snake_case_name}.${ext}",
            "folder": "${difficulty}/id${id}"
        }
    },
    "leetcode.hint.commandShortcut": false,
    "leetcode.hint.commentDescription": false,
    "leetcode.hint.configWebviewMarkdown": false,
    "leetcode.hint.setDefaultLanguage": false,
    "leetcode.useEndpointTranslation": false,
    "leetcode.workspaceFolder": "/Users/charmer/Developer/leetcode-go",
    "lldb.showDisassembly": "never",
    "redhat.telemetry.enabled": false,
    "remote.SSH.remotePlatform": {
        "devcloud": "linux",
        "olarch": "linux"
    },
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.diagnostics.experimental.enable": true,
    "rust-analyzer.imports.granularity.group": "module",
    "rust-analyzer.imports.prefix": "crate",
    "search.exclude": {
        "**/.ammonite": true,
        "**/.bloop": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.metals": true,
        "**/.svn": true,
        "**/dist/**": true,
        "**/node_modules": true,
        "**/node_modules/**": true,
        "**/target": true,
        "**/tmp/**": true
    },
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.fontFamily": "'IosevkaTerm Nerd Font Mono', 'Noto Serif CJK SC'",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.rightClickBehavior": "paste",
    "terminal.integrated.scrollback": 10000,
    "window.autoDetectColorScheme": true,
    "workbench.editor.labelFormat": "short",
    "workbench.iconTheme": "catppuccin-latte",
    "workbench.preferredDarkColorTheme": "Catppuccin Macchiato",
    "workbench.preferredLightColorTheme": "Catppuccin Latte",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 12,
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ]
}